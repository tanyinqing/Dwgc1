<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:background="@color/white_zhende"
    android:gravity="center_vertical"
    android:orientation="horizontal" >

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_weight="2"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/tv_id"
            android:layout_width="28dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:gravity="center"
            android:text="序号"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/tv_name"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:layout_marginLeft="5dp"
            android:gravity="center_vertical|left"
            android:layout_weight="1"
            android:text="档案名称"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#000000"
            android:textSize="15sp" />

        <CheckBox
            android:id="@+id/cb_yes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:button="@drawable/agree_selector"
            android:text="是"
            android:textColor="#000000"
            android:textSize="15sp" />

        <CheckBox
            android:id="@+id/cb_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:button="@drawable/disagree_selector"
            android:text="否"
            android:textColor="#000000"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <ImageButton
            android:id="@+id/bn_detail"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="30dp"
            android:layout_marginLeft="5dp"
            android:background="@null"
            android:src="@drawable/check_biao_zhun"         
          
          />

        <ImageButton
            android:id="@+id/bn_photo"
             android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="30dp"
             android:background="@null"
            android:src="@drawable/check_zhao_xiang"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="5dp" />

        <ImageButton
            android:id="@+id/bn_lookPhoto"
             android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="30dp"
             android:background="@null"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="5dp"
            android:src="@drawable/check_wu" >
        </ImageButton>
    </LinearLayout>

</LinearLayout><!--
